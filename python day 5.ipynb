   "source": [
    "#Count the number of vowels in a string\n",
    "n=input(\"Enter a word: \")\n",
    "count=0\n",
    "for k in n:\n",
    "    if k=='a' or k=='e' or k=='i' or k=='o' or k=='u':\n",
    "        count+=1\n",
    "\n",
    "print(count)\n"
   ]
